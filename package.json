{
	"name": "engeneer",
	"version": "1.0.0",
	"description": "engeneer",
	"main": "main.js",
	"scripts": {
		"dev": "npm run webp && webpack-dev-server --config ./build/webpack.dev.conf.js",
		"build": "npm run webp && webpack --config ./build/webpack.build.conf.js",
		"lint": "eslint --debug src/",
		"lint:write": "eslint --debug src/ --fix",
		"stylelint": "stylelint ./src/**/*.scss",
		"stylelint:write": "stylelint ./src/**/*.scss --fix",
		"webp": "node ./build/webpack.webp.js"
	},
	"browserslist": [
		"> 1%",
		"last 3 version"
	],
	"author": "https://github.com/mderkach",
	"keywords": [
		"webpack",
		"webpack4",
		"merge",
		"babel",
		"babel7",
		"scss",
		"pug"
	],
	"license": "ISC",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.(js|jsx)": [
			"npm run lint:write",
			"git add"
		]
	},
	"devDependencies": {
