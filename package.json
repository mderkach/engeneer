		"eslint-plugin-standard": "^4.0.1",
		"file-loader": "^6.1.0",
		"glob": "^7.1.6",
		"html-webpack-plugin": "^4.4.1",
		"husky": "^4.3.0",
		"image-webpack-loader": "^7.0.0",
		"lint-staged": "^10.4.0",
