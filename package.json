		"css-mqpacker": "^7.0.0",
		"cssnano": "^4.1.10",
		"eslint": "^7.9.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-config-node": "^4.1.0",
